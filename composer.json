{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "creonit/propel-schema-converter-bundle": "^0.2.5",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "propel/propel": "^2.0",
        "propel/propel-bundle": "^5.0",
        "symfony/debug-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-0": {
            "Propel": "vendor/propel/propel/src/"
        },
        "psr-4": {
            "App\\": "src/",
            "Propel\\Bundle\\PropelBundle\\": "vendor/propel/propel-bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "propel/propel",
                "version": "2.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/creonit-dev/Propel2",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "propel/propel-bundle",
                "version": "5.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/creonit-dev/PropelBundle",
                    "reference": "4.0"
                }
            }
        }
    ]
}
